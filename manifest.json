{
	"manifest_version": 3,
	"name": "YoutubeDestroyer",
	"description": "Base(d) Level Extension",
	"permissions": [
		"scripting",
		"tabs",
		"activeTab"
	],
	"version": "0.4.2.0",
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_popup": "index.html",
		"default_icon": "assets/giga_chad.png"
	},
	"host_permissions": [
		"https://www.youtube.com/"
	]
}